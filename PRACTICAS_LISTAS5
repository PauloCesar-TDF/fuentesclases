
# EJERCICIOS VARIOS
numeros = [0,1,4,3,1,4,5,3,7,8,9]
print(numeros[3:6])
print(numeros[:3])
print(numeros[6:])
print(numeros[::2])
print(numeros[::-1])


# CONTAR CUANTAS "A" HAY EN LOS PRIMEROS 5 ELEMENTOS
letras = ['a','b','c','a','a','f','a','f','a']
primeros_5 = letras[:5]
cantidad_letras_a = primeros_5.count("a")

print(f"En los primeros primeros 5 elementos hay {cantidad_letras_a} letras a: ")
